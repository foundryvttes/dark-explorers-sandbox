{"_id":"PftRqd6mDcAKDmdX","name":"Eterófono","type":"cItem","img":"systems/sandbox/docs/icons/sh_citem_icon.png","data":{"ciKey":"PftRqd6mDcAKDmdX","isUnique":true,"uniqueGID":"","usetype":"PAS","selfdestruct":false,"effectpath":"","ispermanent":false,"maxuses":1,"icon":"BOOK","rechargable":false,"roll":"","rollid":"","rollname":"","hasdialog":false,"dialogName":"","dialogID":"","tokeniconpath":"","istokenicon":true,"tokenvisible":false,"mods":[],"groups":[{"id":"dAexvlYVDKedhrVC","name":"item_group","ikey":"item_group"}],"attributes":{"name":"Eterófono","item_name_prop_value":{"value":"Eterófono","ishidden":false},"item_description_prop_value":{"value":"El eterófono o theremin es el primer instrumento musical electrónico. Utilizando principios de electro‐ magnetismo el intérprete solo debe mover sus manos entre dos antenas para producir melodías. En la actualidad algunos investigadores de lo paranormal lo utilizan como sensor de proximidad de presencias.","ishidden":false}},"description":""},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"5O9nsbN12pwBkhtz":3},"flags":{"core":{"sourceId":"Item.SpJhJSFvuhs4vH4i"}}}
